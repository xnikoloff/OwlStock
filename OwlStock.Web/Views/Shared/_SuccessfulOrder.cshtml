@model List<Category>

<div class="container">
    <div class="row mt-5">
        <div class="col-12 col-md-6 col-lg-6 mx-auto text-center">
            <h1>Thank you for your purchase!</h1>
            <h5>The photo will start downloading shortly...</h5>
            <p class="mt-4">See your orders <a asp-action="MyOrders">here</a></p>
            <p>Discover more stunning photos <a asp-action="All" asp-controller="Photo">here</a></p>
            
        </div>
    </div>
    <hr/>
    @await Component.InvokeAsync("PhotoSuggestions", new { category = Model})
</div>