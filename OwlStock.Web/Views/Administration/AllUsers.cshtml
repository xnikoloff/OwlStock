@model IEnumerable<IdentityUser>

<div class="container">
    <div class="row">
        <div class="col-12">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Email</th>
                        <th>Username</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        foreach(IdentityUser user in Model)
                        {
                            <tr onclick="location.href = '@Url.Action("UserById", "Administration", new { id = user.Id })'">
                                <td>@user.Id</td>
                                <td>@user.Email</td>
                                <td>@user.UserName</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>