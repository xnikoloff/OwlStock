@model List<Photo>
<div class="container">
    <div class="row">
        <div class="col-12 col-md-6 col-lg-4">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Name</th>
                        <th>Description</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(Photo photo in Model)
                    {
                        <tr>
                            <td>@photo.Id</td>
                            <td>@photo.Name</td>
                            <td>@photo.Description</td>
                        </tr>
                    }
                    
                </tbody>
            </table>
        </div>
    </div>
</div>